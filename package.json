    "db:undo:seedAll": "npx sequelize-cli db:seed:undo:all",
    "dev:start": "nodemon --exec babel-node ./server/index.js",
    "build": "babel server --out-dir build",
    "test": "echo \"Error: no test specified\" && exit 1"
